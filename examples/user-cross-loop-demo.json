{
  "name": "用户交叉循环示例",
  "id": "user-cross-demo",
  "description": "演示生成用户需求的JSON：key递增，name包含固定值+sadd值+对象字段，其他字段为固定值或对象字段值",
  "count": 1,
  "template": {
    "#nested": {
      "loops": [
        {
          "type": "array",
          "variable": "$obj",
          "array": [
            { "name": "tvoc", "radd": 100 },
            { "name": "pm25", "radd": 200 },
            { "name": "co2", "radd": 300 }
          ]
        },
        {
          "type": "range",
          "variable": "$sadd",
          "start": 1,
          "end": 3,
          "step": 1
        }
      ],
      "template": {
        "key": "{{$index + 1}}",
        "name": "d4ad2034ddess-{{$sadd}}-{{$obj.name}}",
        "sadd": "{{$sadd}}",
        "rtype": 3,
        "radd": "{{$obj.radd}}",
        "dtype": 1,
        "magn": "1",
        "report": 0,
        "ampl": "0"
      }
    }
  }
}