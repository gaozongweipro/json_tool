{
  "name": "json-generator-tool",
  "version": "1.0.0",
  "description": "A flexible JSON generation tool built with Vue 3, can be used locally or embedded as Vue component in other projects",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
