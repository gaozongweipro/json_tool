    "ajv": "^8.12.0",
    "file-saver": "^2.0.5"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/file-saver": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.55.0",
